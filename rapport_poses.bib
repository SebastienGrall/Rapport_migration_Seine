@phdthesis{belliard_peuplement_1994,
	type = {thesis},
	title = {Le {Peuplement} ichtyologique du bassin de la {Seine} : role et signification des {\'e}chelles temporelles et spatiales},
	shorttitle = {Le {Peuplement} ichtyologique du bassin de la {Seine}},
	url = {http://www.theses.fr/1994PA066035},
	abstract = {Ce travail poursuit un double objectif: fournir une synth{\`e}se des donn{\'e}es existantes concernant l'ichtyocoenose du bassin de la seine et, au del{\`a}, comprendre les modalit{\'e}s d'organisation des peuplements piscicoles a l'{\'e}chelle d'un bassin fluvial. La premi{\`e}re partie aborde, a travers plusieurs niveaux de perception temporelle, les aspects relatifs a l'{\'e}volution des peuplements piscicoles. L'origine biog{\'e}ographique de la faune est rappel{\'e}e. Le poids des activit{\'e}s humaines sur la composition de l'ichtyocoenose actuelle est souligne. Enfin, l'{\'e}volution de la structure des peuplements depuis le milieu du si{\`e}cle dernier est d{\'e}taill{\'e}e pour quelques secteurs du bassin. La seconde partie analyse l'organisation actuelle des peuplements a l'{\'e}chelle du bassin en liaison avec diff{\'e}rents param{\`e}tres de l'environnement. Deux facteurs structurants sont plus particuli{\`e}rement examin{\'e}s: le gradient longitudinal du r{\'e}seau fluvial responsable d'une succession d'esp{\`e}ces de l'amont vers l'aval, et l'organisation r{\'e}gionale du bassin qui conduit a une diff{\'e}renciation des peuplements issus de rivi{\`e}res traversant des r{\'e}gions naturelles diff{\'e}rentes. La conclusion insiste sur l'importance de prendre en compte la dimension historique dans l'{\'e}tude des peuplements et sur la complexit{\'e} de l'expression de la diversit{\'e} des ichtyocoenoses en milieu fluvial},
	urldate = {2019-01-24},
	school = {Paris 6},
	author = {Belliard, J{\'e}r{\^o}me},
	month = jan,
	year = {1994},
	keywords = {Biodiversit{\'e} -- France -- Seine, Bassin de la (France), Hydrologie, Ichtyologie -- France -- Seine, Bassin de la (France), Poissons -- Populations -- France -- Seine, Bassin de la (France)},
	file = {Snapshot:D\:\\OneDrive - Seine-Normandie Migrateurs\\Zotero\\storage\\Q3HA9C4X\\1994PA066035.html:text/html},
}

